            <android.support.constraint.ConstraintLayout
                android:id="@+id/copyRightMenu"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:clickable="true"
                android:orientation="horizontal"
                android:paddingStart="@dimen/marginTen"
                android:paddingTop="@dimen/marginTen"

                >

                <ImageView
                    android:id="@+id/imageViewcopyRightMenu"
                    android:layout_width="0dp"
                    android:layout_height="60dp"

                    android:adjustViewBounds="true"
                    android:scaleType="fitCenter"
                    android:src="@drawable/copy_right_icon"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


                <RelativeLayout
                    android:id="@+id/versionLayout"
                    android:layout_width="0dp"
                    android:layout_height="59dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/icon_background"
                    android:clickable="true"
                    android:cropToPadding="true"
                    android:scaleType="fitEnd"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/imageViewcopyRightMenu"
                    app:layout_constraintTop_toTopOf="parent">

                    <ImageView
                        android:id="@+id/imageView3copyRightMenu"
                        android:layout_width="12dp"
                        android:layout_height="12dp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="19dp"
                        android:src="@drawable/ic_next_arrow"
                        android:tint="@color/blackDim"
